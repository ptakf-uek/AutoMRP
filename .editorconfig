# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 4
quote_type = double
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = true
spaces_around_operators = true
indent_brace_style= K&R
